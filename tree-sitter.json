{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "scone",
      "camelcase": "Scone",
      "title": "Scone",
      "scope": "source.scone",
      "file-types": [
        "scone"
      ],
      "injection-regex": "^scone$",
      "class-name": "TreeSitterScone",
      "highlights": [
        "queries/highlights.scm"
      ]
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "The Scone Language",
    "authors": [
      {
        "name": "FL4SHK"
      }
    ],
    "links": {
      "repository": "https://github.com/fl4shk/tree-sitter-scone"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
